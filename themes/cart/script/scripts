 $("#demoMenu").aceResponsiveMenu();
 $(document).ready(function(){
    $("#cart-link").click(function(){
        $(".cart-wrapper").slideToggle("slow");
    });

    $("#tab1").click(function() {
    $('html,body').animate({
        scrollTop: $(".student-cta1").offset().top},
        'slow');
    });
    $("#tab2").click(function() {
    $('html,body').animate({
        scrollTop: $(".right-choice").offset().top},
        'slow');
    });

});
